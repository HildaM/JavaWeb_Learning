<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <com.quan.servlet>
        <com.quan.servlet-name>redirect</com.quan.servlet-name>
        <com.quan.servlet-class>com.quan.com.quan.servlet.RedirectServlet</com.quan.servlet-class>
    </com.quan.servlet>
    <com.quan.servlet-mapping>
        <com.quan.servlet-name>redirect</com.quan.servlet-name>
        <url-pattern>/redirect</url-pattern>
    </com.quan.servlet-mapping>
    
    
    <com.quan.servlet>
        <com.quan.servlet-name>image</com.quan.servlet-name>
        <com.quan.servlet-class>com.quan.com.quan.servlet.FileServlet</com.quan.servlet-class>
    </com.quan.servlet>
    <com.quan.servlet-mapping>
        <com.quan.servlet-name>image</com.quan.servlet-name>
        <url-pattern>/image</url-pattern>
    </com.quan.servlet-mapping>
    
    
    <com.quan.servlet>
        <com.quan.servlet-name>randomNum</com.quan.servlet-name>
        <com.quan.servlet-class>com.quan.com.quan.servlet.ImageServlet</com.quan.servlet-class>
    </com.quan.servlet>
    <com.quan.servlet-mapping>
        <com.quan.servlet-name>randomNum</com.quan.servlet-name>
        <url-pattern>/randomNum</url-pattern>
    </com.quan.servlet-mapping>
    
    
    <com.quan.servlet>
        <com.quan.servlet-name>request</com.quan.servlet-name>
        <com.quan.servlet-class>com.quan.com.quan.servlet.RequestTest</com.quan.servlet-class>
    </com.quan.servlet>
    <com.quan.servlet-mapping>
        <com.quan.servlet-name>request</com.quan.servlet-name>
        <url-pattern>/login</url-pattern>
    </com.quan.servlet-mapping>
</web-app>